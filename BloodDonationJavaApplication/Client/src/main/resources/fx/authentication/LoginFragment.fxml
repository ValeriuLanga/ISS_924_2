<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root minHeight="400.0" minWidth="300.0" prefHeight="400.0" prefWidth="300.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox alignment="TOP_CENTER" minHeight="400.0" minWidth="300.0" prefHeight="400.0" prefWidth="300.0">
            <children>
                <Label alignment="CENTER" minHeight="40.0" minWidth="-Infinity" prefHeight="40.0" text="LOGIN" textAlignment="CENTER" VBox.vgrow="ALWAYS">
                    <font>
                        <Font name="Trebuchet MS" size="30.0" />
                    </font>
                </Label>
                <HBox prefHeight="70.0" prefWidth="200.0" />
                <HBox alignment="CENTER" prefHeight="40.0" prefWidth="200.0">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                    <children>
                        <Label minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="80.0" text="Username: " />
                        <TextField fx:id="userNameField" minHeight="25.0" minWidth="40.0" prefHeight="25.0" />
                    </children>
                </HBox>
                <HBox alignment="CENTER" layoutX="10.0" layoutY="116.0" prefHeight="40.0" prefWidth="200.0">
                    <children>
                        <Label minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="80.0" text="Password: " />
                        <PasswordField fx:id="passwordField" minHeight="25.0" minWidth="40.0" prefHeight="25.0" />
                    </children>
                </HBox>
            <HBox alignment="CENTER" prefHeight="40.0" prefWidth="200.0">
               <children>
                  <Label fx:id="errorLabel" />
               </children>
            </HBox>
            </children>
        </VBox>
    </children>
</fx:root>
